@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



    <div>

        <table class="table table-bordered table-responsive table-hover">
            <tr>
                <td class="col-lg-1">Student ID</td>
                <td class="col-lg-1">Student Name</td>
                <td class="col-lg-1">Student Email</td>
                <td class="col-lg-3">Operation</td>
            </tr>
            <tr>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.userID</td>
                    <td>@item.userName</td>
                    <td>@item.email</td>

                </tr>
            }
        </table>

    </div>


